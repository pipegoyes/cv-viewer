{
  "routes": [
      {
          "route": "*.{js,json,css,jpg,png,ico,webmanifest}"
      },
      {
          "route": "/index.html",
          "rewrite": "/en-US/index.html"
      },
      {
          "route": "/en-US/*",
          "rewrite": "/en-US/index.html"
      },
      {
          "route": "/de/*",
          "rewrite": "/de/index.html"
      }
  ]
}