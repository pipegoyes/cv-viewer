{
    "routes": [
      {
        "route": "/de/*",
        "rewrite": "/de/index.html"
      },
      {
        "route": "/en-US/*",
        "rewrite": "/en-US/index.html"
      },
      {
        "route": "/",
        "allowedRoles": ["anonymous"],
        "headers": {
          "x-msw-language": "{header.Accept-Language}"
        }
      },
      {
        "route": "/",
        "redirect": "/de/",
        "headers": {
          "x-msw-language": "{header.Accept-Language}",
          "x-msw-check-language": "^de"
        }
      },
      {
        "route": "/",
        "redirect": "/en/",
        "headers": {
          "x-msw-language": "{header.Accept-Language}",
          "x-msw-check-language": "^en"
        }
      },
      {
        "route": "/",
        "redirect": "/en/",
        "headers": {
          "x-msw-language": "{header.Accept-Language}",
          "x-msw-check-language": "!^en"
        }
      },
      {
        "route": "/*",
        "rewrite": "/en-US/index.html"
      }
    ]
  }
  